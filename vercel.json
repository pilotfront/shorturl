{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}


{
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.pilotfront.com",
      "permanent": true
    }
  ]
}
